        <Label fx:id="output" layoutX="145.0" layoutY="237.0" text="?output" />
        <TextField fx:id="frameInput" layoutX="192.0" layoutY="100.0" prefHeight="26.0" prefWidth="75.0" />
        <TextField fx:id="pagerefInput" layoutX="192.0" layoutY="146.0" prefHeight="26.0" prefWidth="317.0" />
