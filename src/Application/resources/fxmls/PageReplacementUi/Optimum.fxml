<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Application.controllers.PageReplacement.OptimumController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button layoutX="52.0" layoutY="32.0" mnemonicParsing="false" onAction="#goToBack" text="Back" />
        <Button layoutX="48.0" layoutY="324.0" mnemonicParsing="false" onAction="#goToHome" text="Home" />
        <Label layoutX="200.0" layoutY="23.0" text="Optimum page replacement algorithm" />
